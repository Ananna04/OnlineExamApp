<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.onlineexamapplication2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/exam_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/exam_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".StartExamActivity" />
        <activity android:name=".Result_Activity" />
        <activity android:name=".LoginPage" />
        <activity android:name=".AboutUsPage"></activity>
    </application>

</manifest>